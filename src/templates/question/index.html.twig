{% extends 'base.html.twig' %}

{% block title %}
  question_index
{% endblock %}

{% block body %}
  <div>
    {{ include('components/navbar.html.twig') }}
  </div>
  
  <div class="default-container">
    {% for question in questions %}
      <p>{{ question.id }}</p>
      <p>{{ question.title }}</p>
      <p>{{ question.content }}</p>
      <p>{{ question.createdAt|ago }}</p>
      <br>
      <br>
    {% endfor %}
  </div>
{% endblock %}
